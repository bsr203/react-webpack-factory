{
  "name": "react-webpack-starter",
  "version": "0.2.0",
  "description": "React Webpack Starter",
  "main": "src/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development node webpack.server.js",
    "test": "npm run test-eslint && npm run test-karma",
    "test-eslint": "cross-env NODE_ENV=test eslint src",
    "test-karma": "cross-env NODE_ENV=test karma start test/karma.conf.js",
    "test:watch": "npm run test -- --no-single-run",
    "build": "rimraf dist/*; cross-env NODE_ENV=production UV_THREADPOOL_SIZE=100 webpack"
  },
  "author": "Olivier Louvignes <olivier@mg-crea.com>",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "es6-promise": "^3.2.1",
    "isomorphic-fetch": "^2.2.1",
    "normalize.css": "^4.2.0",
    "react": "^15.2.1",
    "react-addons-css-transition-group": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.5",
    "react-toolbox": "^1.0.2",
    "redux": "^3.5.2",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-rest-resource": "^0.6.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015-native-modules": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-runtime": "^6.9.2",
    "browser-sync": "^2.13.0",
    "connect-modrewrite": "^0.9.0",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "eslint-config-airbnb-base": "^4.0.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-import-resolver-webpack": "^0.3.2",
    "eslint-plugin-import": "^1.10.2",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "eslint": "^3.0.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.1",
    "html-loader": "^0.4.3",
    "node-sass": "3.8.0",
    "postcss-loader": "^0.9.1",
    "progress-bar-webpack-plugin": "^1.8.0",
    "react-hot-loader": "^3.0.0-beta.2",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.1",
    "webpack": "^2.1.0-beta.17"
  }
}
